{
  "name": "react-drive-gallery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
  "dev": "node scripts/generateStorageIndex.js && vite",
  "build": "node scripts/generateStorageIndex.js && vite build",
  "build:ghpages": "node scripts/generateStorageIndex.js && vite build",
  "build:docs": "node scripts/generateStorageIndex.js && vite build --base /recipes/ && rm -rf docs && cp -R dist docs",
  "preview": "node scripts/generateStorageIndex.js && vite preview",
    "generate:storage-index": "node scripts/generateStorageIndex.js"
    ,"watch:storage": "node scripts/watchStorage.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "typescript": "^5.5.0",
    "vite": "^5.2.0"
  }
}
